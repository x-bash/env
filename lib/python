
xenv(){
    param <<A
Subcommands:
    use|activate|act
    unuse|deactivate|dea

    local
    global
    shell

    install
    uninstall

    rehash
    version

    exec

    ls|versions
    lsremote|lsr

    alias
    unalias

    which
    whence

    cache
A

}

